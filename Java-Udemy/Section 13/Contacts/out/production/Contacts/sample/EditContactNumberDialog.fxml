<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.EditContactNumberDialogController">
    <headerText>
        Edit the selected contact
        type use previous if you want to use the previous contact data
        press cancel if you want to exit
    </headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="New Number" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="contactNumber" GridPane.rowIndex="0" GridPane.columnIndex="1"/>

        </GridPane>
    </content>
</DialogPane>
